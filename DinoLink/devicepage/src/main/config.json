{
  "app": {
    "bundleName": "com.dinolink.dinolink",
    "vendor": "dinolink",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.dinolink.dinolink",
    "name": ".MyApplication",
    "mainAbility": "com.dinolink.dinolink.devicePage",
    "deviceType": [
      "phone",
      "tablet",
      "wearable"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "devicepage",
      "moduleType": "feature",
      "installationFree": false
    },
    "abilities": [
      {
        "visible": true,
        "name": "com.dinolink.dinolink.devicePage",
        "icon": "$media:icon",
        "description": "$string:devicepage_description",
        "formsEnabled": true,
        "label": "$string:devicepage_devicePage",
        "type": "page",
        "forms": [
          {
            "jsComponentName": "widget",
            "isDefault": true,
            "scheduledUpdateTime": "10:30",
            "defaultDimension": "2*2",
            "name": "widget",
            "description": "This is a service widget",
            "colorMode": "auto",
            "type": "JS",
            "supportDimensions": [
              "2*2"
            ],
            "updateEnabled": true,
            "updateDuration": 1
          }
        ],
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      },
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "widget",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        },
        "type": "form"
      }
    ]
  }
}